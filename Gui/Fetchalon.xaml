                                                <TextBlock.Style>
                                                    <Style TargetType="TextBlock"
                                                           BasedOn="{StaticResource PropTitleStyle}">
                                                        <Setter Property="FontSize"
                                                                Value="13" />
                                                        <Setter Property="Grid.Column"
                                                                Value="2" />
                                                        <Setter Property="Text"
                                                                Value="{Binding Path=Source}" />
                                                    </Style>
                                                </TextBlock.Style>
                                            </TextBlock>
                                            <Button>
                                                <Button.Style>
                                                    <Style TargetType="Button"
                                                           BasedOn="{StaticResource BtnRunPropStyle}">
                                                        <Setter Property="Content"
                                                                Value="{Binding Path=Handler.Title}" />
                                                        <Setter Property="Grid.Column"
                                                                Value="3" />
                                                        <Setter Property="Padding"
                                                                Value="5,0" />
                                                        <Setter Property="Visibility"
                                                                Value="Visible" />
                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{Binding Path=Handler}"
                                                                         Value="{x:Null}">
                                                                <Setter Property="Visibility"
                                                                        Value="Hidden" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </Button.Style>
                                            </Button>
                                            <Button>
                                                <Button.Style>
                                                    <Style TargetType="Button"
                                                           BasedOn="{StaticResource BtnCopyPropertyStyle}" />
                                                </Button.Style>
                                            </Button>
                                            <ProgressBar>
                                                <ProgressBar.Style>
                                                    <Style TargetType="ProgressBar">
                                                        <Setter Property="Grid.ColumnSpan"
                                                                Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=ColumnDefinitions.Count}" />
                                                        <Setter Property="Grid.Row"
                                                                Value="1" />
                                                        <Setter Property="Height"
                                                                Value="8" />
                                                        <Setter Property="IsIndeterminate"
                                                                Value="False" />
                                                        <Setter Property="Maximum"
                                                                Value="{Binding Path=Handler.ProgressMax}" />
                                                        <Setter Property="Visibility"
                                                                Value="Visible" />
                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{Binding Path=Handler.Progress}"
                                                                         Value="-1">
                                                                <Setter Property="IsIndeterminate"
                                                                        Value="True" />
                                                                <Setter Property="Visibility"
                                                                        Value="Visible" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Path=Handler.Progress}"
                                                                         Value="0">
                                                                <Setter Property="Visibility"
                                                                        Value="Collapsed" />
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Path=Handler.Progress}"
                                                                         Value="{x:Null}">
                                                                <Setter Property="Visibility"
                                                                        Value="Collapsed" />
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </ProgressBar.Style>
                                            </ProgressBar>
                                            <ContentControl>
                                                <ContentControl.Style>
                                                    <Style TargetType="ContentControl">
                                                        <Setter Property="ContentTemplate">
                                                            <Setter.Value>
                                                                <DataTemplate>
                                                                    <TextBlock>
                                                                        <TextBlock.Style>
                                                                            <Style TargetType="TextBlock"
                                                                                   BasedOn="{StaticResource TblPropListItemStyle}">
                                                                                <Setter Property="Text"
                                                                                        Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.Value}" />
                                                                                <Setter Property="Margin"
                                                                                        Value="0" />
                                                                            </Style>
                                                                        </TextBlock.Style>
                                                                    </TextBlock>
                                                                </DataTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                        <Setter Property="Grid.ColumnSpan"
                                                                Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=ColumnDefinitions.Count}" />
                                                        <Setter Property="Grid.Row"
                                                                Value="2" />
                                                        <Style.Triggers>
                                                            <DataTrigger Binding="{Binding Path=Type}"
                                                                         Value="ArrayList">
                                                                <Setter Property="ContentTemplate">
                                                                    <Setter.Value>
                                                                        <DataTemplate>
                                                                            <ScrollViewer>
                                                                                <ScrollViewer.Style>
                                                                                    <Style TargetType="ScrollViewer">
                                                                                        <Setter Property="DataContext"
                                                                                                Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext}" />
                                                                                    </Style>
                                                                                </ScrollViewer.Style>
                                                                                <ItemsControl>
                                                                                    <ItemsControl.Resources>
                                                                                        <CollectionViewSource x:Key="CvsItems"
                                                                                                              Source="{Binding RelativeSource={RelativeSource AncestorType=ScrollViewer}, Path=DataContext.Value}">
                                                                                            <CollectionViewSource.SortDescriptions>
                                                                                                <componentmodel:SortDescription Direction="Ascending" />
                                                                                            </CollectionViewSource.SortDescriptions>
                                                                                        </CollectionViewSource>
                                                                                        <Style TargetType="Label"
                                                                                               x:Key="LblItemStyle">
                                                                                            <Setter Property="Background"
                                                                                                    Value="White" />
                                                                                            <Setter Property="Margin"
                                                                                                    Value="0" />
                                                                                            <Setter Property="VerticalAlignment"
                                                                                                    Value="Center" />
                                                                                            <Style.Triggers>
                                                                                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=Self}, Path=IsMouseOver}"
                                                                                                             Value="True">
                                                                                                    <Setter Property="Background"
                                                                                                            Value="PowderBlue" />
                                                                                                </DataTrigger>
                                                                                            </Style.Triggers>
                                                                                        </Style>
                                                                                        <Style TargetType="TextBlock"
                                                                                               x:Key="TblItemStyle">
                                                                                            <Setter Property="Background"
                                                                                                    Value="White" />
                                                                                            <Setter Property="Padding"
                                                                                                    Value="{StaticResource StdPadding}" />
                                                                                            <Setter Property="TextWrapping"
                                                                                                    Value="Wrap" />
                                                                                            <Setter Property="VerticalAlignment"
                                                                                                    Value="Center" />
                                                                                            <Style.Triggers>
                                                                                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=Self}, Path=IsMouseOver}"
                                                                                                             Value="True">
                                                                                                    <Setter Property="Background"
                                                                                                            Value="PowderBlue" />
                                                                                                </DataTrigger>
                                                                                            </Style.Triggers>
                                                                                        </Style>
                                                                                        <DataTemplate x:Key="BoolTemplate">
                                                                                            <TextBlock>
                                                                                                <TextBlock.Style>
                                                                                                    <Style TargetType="TextBlock"
                                                                                                           BasedOn="{StaticResource TblItemStyle}">
                                                                                                        <Setter Property="Text"
                                                                                                                Value="{Binding Path=.}" />
                                                                                                    </Style>
                                                                                                </TextBlock.Style>
                                                                                            </TextBlock>
                                                                                        </DataTemplate>
                                                                                        <DataTemplate x:Key="DateTimeTemplate">
                                                                                            <Label>
                                                                                                <Label.Style>
                                                                                                    <Style TargetType="Label"
                                                                                                           BasedOn="{StaticResource LblItemStyle}">
                                                                                                        <Setter Property="Content"
                                                                                                                Value="{Binding Path=.}" />
                                                                                                        <Setter Property="ContentStringFormat"
                                                                                                                Value="{DynamicResource StrFullDateTimeFormat}" />
                                                                                                        <Setter Property="Margin"
                                                                                                                Value="0" />
                                                                                                    </Style>
                                                                                                </Label.Style>
                                                                                            </Label>
                                                                                        </DataTemplate>
                                                                                        <DataTemplate x:Key="DefaultTemplate">
                                                                                            <TextBlock>
                                                                                                <TextBlock.Style>
                                                                                                    <Style TargetType="TextBlock"
                                                                                                           BasedOn="{StaticResource TblItemStyle}">
                                                                                                        <Setter Property="Text"
                                                                                                                Value="{Binding Path=.}" />
                                                                                                    </Style>
                                                                                                </TextBlock.Style>
                                                                                            </TextBlock>
                                                                                        </DataTemplate>
                                                                                        <DataTemplate x:Key="GroupTemplate">
                                                                                            <TextBlock>
                                                                                                <TextBlock.Style>
                                                                                                    <Style TargetType="TextBlock"
                                                                                                           BasedOn="{StaticResource TblItemStyle}">
                                                                                                        <Setter Property="Text"
                                                                                                                Value="{Binding Path=.}" />
                                                                                                    </Style>
                                                                                                </TextBlock.Style>
                                                                                            </TextBlock>
                                                                                        </DataTemplate>
                                                                                        <DataTemplate x:Key="StringTemplate"
                                                                                                      DataType="{x:Type sys:String}">
                                                                                            <TextBlock>
                                                                                                <TextBlock.Style>
                                                                                                    <Style TargetType="TextBlock"
                                                                                                           BasedOn="{StaticResource TblItemStyle}">
                                                                                                        <Setter Property="Text"
                                                                                                                Value="{Binding Path=.}" />
                                                                                                    </Style>
                                                                                                </TextBlock.Style>
                                                                                            </TextBlock>
                                                                                        </DataTemplate>
                                                                                        <DataTemplate x:Key="UserTemplate">
                                                                                            <TextBlock>
                                                                                                <TextBlock.Style>
                                                                                                    <Style TargetType="TextBlock"
                                                                                                           BasedOn="{StaticResource TblItemStyle}">
                                                                                                        <Setter Property="Text">
                                                                                                            <Setter.Value>
                                                                                                                <PriorityBinding>
                                                                                                                    <Binding Path="."
                                                                                                                             IsAsync="True"
                                                                                                                             Mode="OneTime" />
                                                                                                                    <Binding Path="." />
                                                                                                                </PriorityBinding>
                                                                                                            </Setter.Value>
                                                                                                        </Setter>
                                                                                                    </Style>
                                                                                                </TextBlock.Style>
                                                                                            </TextBlock>
                                                                                        </DataTemplate>
                                                                                        <c:IcItemTemplateSelector x:Key="DataTemplateSelector"
                                                                                                                  BoolTpl="{StaticResource BoolTemplate}"
                                                                                                                  DateTimeTpl="{StaticResource DateTimeTemplate}"
                                                                                                                  DefaultTpl="{StaticResource DefaultTemplate}"
                                                                                                                  GroupTpl="{StaticResource GroupTemplate}"
                                                                                                                  StringTpl="{StaticResource StringTemplate}"
                                                                                                                  UserTpl="{StaticResource UserTemplate}" />
                                                                                    </ItemsControl.Resources>
                                                                                    <ItemsControl.Style>
                                                                                        <Style TargetType="ItemsControl"
                                                                                               BasedOn="{StaticResource IcBaseStyle}">
                                                                                            <Setter Property="ItemsSource"
                                                                                                    Value="{Binding Source={StaticResource CvsItems}}" />
                                                                                            <Setter Property="ItemTemplateSelector"
                                                                                                    Value="{StaticResource DataTemplateSelector}" />
                                                                                            <Setter Property="ScrollViewer.VerticalScrollBarVisibility"
                                                                                                    Value="Auto" />
                                                                                            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility"
                                                                                                    Value="Auto" />
                                                                                            <Setter Property="VirtualizingPanel.IsVirtualizing"
                                                                                                    Value="True" />
                                                                                            <Setter Property="VirtualizingPanel.VirtualizationMode"
                                                                                                    Value="Recycling" />
                                                                                            <Setter Property="VirtualizingPanel.IsManipulationEnabled"
                                                                                                    Value="False" />
                                                                                        </Style>
                                                                                    </ItemsControl.Style>
                                                                                </ItemsControl>
                                                                            </ScrollViewer>
                                                                        </DataTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Path=Type}"
                                                                         Value="ObjectList">
                                                                <Setter Property="ContentTemplate">
                                                                    <Setter.Value>
                                                                        <DataTemplate>
                                                                            <DataGrid>
                                                                                <DataGrid.Style>
                                                                                    <Style TargetType="DataGrid"
                                                                                           BasedOn="{StaticResource DgPropValueStyle}">
                                                                                        <Setter Property="ItemsSource"
                                                                                                Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.Value}" />
                                                                                    </Style>
                                                                                </DataGrid.Style>
                                                                            </DataGrid>
                                                                        </DataTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Path=Type}"
                                                                         Value="DateTime">
                                                                <Setter Property="ContentTemplate">
                                                                    <Setter.Value>
                                                                        <DataTemplate>
                                                                            <Label>
                                                                                <Label.Style>
                                                                                    <Style TargetType="Label"
                                                                                           BasedOn="{StaticResource LblPropStringValueStyle}">
                                                                                        <Setter Property="Content"
                                                                                                Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.Value}" />
                                                                                        <Setter Property="ContentStringFormat"
                                                                                                Value="{DynamicResource StrFullDateTimeFormat}" />
                                                                                        <Setter Property="Margin"
                                                                                                Value="0" />
                                                                                    </Style>
                                                                                </Label.Style>
                                                                            </Label>
                                                                        </DataTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Path=Type}"
                                                                         Value="Boolean">
                                                                <Setter Property="ContentTemplate">
                                                                    <Setter.Value>
                                                                        <DataTemplate>
                                                                            <Label>
                                                                                <Label.Style>
                                                                                    <Style TargetType="Label"
                                                                                           BasedOn="{StaticResource LblPropStringValueStyle}">
                                                                                        <Setter Property="Content"
                                                                                                Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.Value}" />
                                                                                        <Setter Property="Margin"
                                                                                                Value="0" />
                                                                                    </Style>
                                                                                </Label.Style>
                                                                            </Label>
                                                                        </DataTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding Path=Type}"
                                                                         Value="Int64">
                                                                <Setter Property="ContentTemplate">
                                                                    <Setter.Value>
                                                                        <DataTemplate>
                                                                            <Label>
                                                                                <Label.Style>
                                                                                    <Style TargetType="Label"
                                                                                           BasedOn="{StaticResource LblPropStringValueStyle}">
                                                                                        <Setter Property="Content"
                                                                                                Value="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=DataContext.Value}" />
                                                                                        <Setter Property="Margin"
                                                                                                Value="0" />
                                                                                    </Style>
                                                                                </Label.Style>
                                                                            </Label>
                                                                        </DataTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </DataTrigger>
                                                        </Style.Triggers>
                                                    </Style>
                                                </ContentControl.Style>
                                            </ContentControl>
                                        </Grid>
                                    </Border>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </Grid>
                </ScrollViewer>
            </Grid>
            <Grid x:Name="GridFailedSearch">
                <Grid.Style>
                    <Style TargetType="Grid">
                        <Setter Property="Grid.Column"
                                Value="3" />
                        <Setter Property="Grid.Row"
                                Value="1" />
                        <Setter Property="Visibility"
                                Value="Collapsed" />
                    </Style>
                </Grid.Style>
                <Grid.Resources>
                    <sys:Double x:Key="IconSize">100</sys:Double>
                </Grid.Resources>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Image>
                    <Image.Style>
                        <Style TargetType="Image">
                            <Setter Property="Source">
                                <Setter.Value>
                                    <DrawingImage>
                                        <DrawingImage.Drawing>
                                            <GeometryDrawing Geometry="{StaticResource FailedSearchIcon}">
                                                <GeometryDrawing.Pen>
                                                    <Pen Thickness="2"
                                                         Brush="Black" />
                                                </GeometryDrawing.Pen>
                                                <GeometryDrawing.Brush>
                                                    <SolidColorBrush Color="LightCoral" />
                                                </GeometryDrawing.Brush>
                                            </GeometryDrawing>
                                        </DrawingImage.Drawing>
                                    </DrawingImage>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="Height"
                                    Value="{StaticResource IconSize}" />
                            <Setter Property="HorizontalAlignment"
                                    Value="Center" />
                            <Setter Property="VerticalAlignment"
                                    Value="Bottom" />
                            <Setter Property="Width"
                                    Value="{StaticResource IconSize}" />
                        </Style>
                    </Image.Style>
                </Image>
                <TextBlock>
                    <TextBlock.Style>
                        <Style TargetType="TextBlock">
                            <Setter Property="FontSize"
                                    Value="20" />
                            <Setter Property="Grid.Row"
                                    Value="1" />
                            <Setter Property="HorizontalAlignment"
                                    Value="Center" />
                            <Setter Property="Text"
                                    Value="{Binding ElementName=Window, Path=DataContext.MsgTable.ContentTblFailedSearchText}" />
                            <Setter Property="VerticalAlignment"
                                    Value="Top" />
                        </Style>
                    </TextBlock.Style>
                </TextBlock>
            </Grid>
            <Frame Name="FrameTool">
                <Frame.Style>
                    <Style TargetType="Frame">
                        <Setter Property="NavigationUIVisibility"
                                Value="Hidden" />
                        <Setter Property="Grid.Column"
                                Value="3" />
                        <Setter Property="Grid.Row"
                                Value="1" />
                        <Setter Property="ScrollViewer.VerticalScrollBarVisibility"
                                Value="Auto" />
                        <Setter Property="Visibility"
                                Value="Collapsed" />
                    </Style>
                </Frame.Style>
            </Frame>
        </Grid>
    </Grid>
</Window>
