            <Setter Property="Content"
                    Value="{Binding ElementName=Window, Path=DataContext.MsgTable.ContentBtnFetch}" />
            <Setter Property="Grid.Column"
                    Value="1" />
        </Style>
        <Style TargetType="Button"
               x:Key="BtnRunPropStyle"
               BasedOn="{StaticResource BtnBaseStyle}">
            <EventSetter Event="Click" />
        </Style>
        <Style TargetType="Button"
               x:Key="PropSaveValueStyle"
               BasedOn="{StaticResource BtnBaseStyle}">
            <Setter Property="Content"
                    Value="{Binding ElementName=Window, Path=DataContext.MsgTable.ContentSavePropValueButton}" />
            <Setter Property="Grid.Column"
                    Value="1" />
            <Setter Property="Grid.Row"
                    Value="1" />
        </Style>
        <Style TargetType="Button"
               x:Key="BtnViewFileDir">
            <EventSetter Event="Click" />
            <Setter Property="Content"
                    Value="{Binding ElementName=Window, Path=DataContext.MsgTable.ContentBtnFileView}" />
            <Setter Property="Grid.Column"
                    Value="2" />
            <Setter Property="Visibility"
                    Value="Collapsed" />
            <Style.Triggers>
                <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Border}, Path=DataContext.Type}"
                             Value="Directory">
                    <Setter Property="Content"
                            Value="{Binding ElementName=Window, Path=DataContext.MsgTable.ContentBtnDirView}" />
